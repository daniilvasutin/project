<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        version="3.0">

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>main.java.servletPack.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddCompany</servlet-name>
        <servlet-class>main.java.servletPack.AddCompany</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCompany</servlet-name>
        <url-pattern>/AddCompany/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Enter</servlet-name>
        <servlet-class>main.java.servletPack.Enter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Enter</servlet-name>
        <url-pattern>/Enter/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EnterByAName</servlet-name>
        <servlet-class>main.java.servletPack.EnterByAName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EnterByAName</servlet-name>
        <url-pattern>/EnterByAName/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddProject</servlet-name>
        <servlet-class>main.java.servletPack.AddProject</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddProject</servlet-name>
        <url-pattern>/AddProject/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowCompanyProject</servlet-name>
        <servlet-class>main.java.servletPack.ShowCompanyProject</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowCompanyProject</servlet-name>
        <url-pattern>/ShowCompanyProject/*</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>./jsp/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
